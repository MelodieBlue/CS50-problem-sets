\\ For printing: Hello,world

#include <stdio.h>
#include <cs50.h>

int main(void)
{
    printf("Hello, world\n");
}

\\ For printing 10 times: Hello,world

#include <stdio.h>

for(int i = 0; i<10; i++)
{
    printf("Hello,world\n");
}

\\ For printing: Hello, _____ (name)

#include <cs50.h>
#include <stdio.h>

int main(void)

{
     char string_name = getchar();
     printf("Hello, string_name\n");
}

\\
